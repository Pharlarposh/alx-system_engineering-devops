#!/usr/bin/env bash
# This script creates an RSA key pair

# Create RSA key pair with 4096 bits and passphrase 'betty'
ssh-keygen -t rsa -b 4096 -f ~/.ssh/school -C "Generated by script" -N 'betty'

# Move the public key to school.pub
mv ~/.ssh/school.pub ~/.ssh/school.pub

echo "RSA key pair created successfully."
